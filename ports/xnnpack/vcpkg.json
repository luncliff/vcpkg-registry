{
  "name": "xnnpack",
  "version-date": "2025-06-21",
  "description": "High-efficiency floating-point neural network inference operators for mobile, server, and Web",
  "homepage": "https://github.com/google/XNNPACK",
  "license": "BSD-3-Clause",
  "dependencies": [
    "cpuinfo",
    "fp16",
    "fxdiv",
    "google-pthreadpool",
    {
      "name": "vcpkg-cmake",
      "host": true
    }
  ],
  "features": {
    "kleidiai": {
      "description": "Use KleidiAI GEMM microkernels for Arm",
      "supports": "arm64",
      "dependencies": [
        "kleidiai"
      ]
    },
    "test": {
      "description": "Build and run tests",
      "dependencies": [
        "benchmark",
        "gtest"
      ]
    }
  }
}
