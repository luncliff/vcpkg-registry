{
  "name": "mediapipe",
  "version-semver": "0.8.8",
  "description": "Cross-platform, customizable ML solutions for live and streaming media.",
  "homepage": "https://mediapipe.dev",
  "supports": "windows",
  "dependencies": [
    "glog",
    "stb",
    "protobuf",
    "tensorflow",
    "abseil",
    "eigen3",
    {
      "name": "pthreads",
      "platform": "windows"
    },
    {
      "name": "egl-registry",
      "platform": "windows"
    },
    {
      "name": "opengl-registry",
      "platform": "windows"
    },
    {
      "name": "ceres",
      "features": [
        "eigensparse"
      ]
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "opencv4"
  ],
  "features": {
    "opencv4": {
      "description": "Build with OpenCV 4",
      "dependencies": [
        "opencv4"
      ]
    }
  }
}