{
  "name": "mediapipe",
  "version-semver": "0.8.9",
  "description": "Cross-platform, customizable ML solutions for live and streaming media.",
  "homepage": "https://mediapipe.dev",
  "supports": "windows",
  "dependencies": [
    "abseil",
    {
      "name": "angle",
      "platform": "windows"
    },
    {
      "name": "ceres",
      "features": [
        "eigensparse"
      ]
    },
    "eigen3",
    "glog",
    "libyuv",
    "protobuf",
    {
      "name": "pthreads",
      "platform": "windows"
    },
    "stb",
    {
      "name": "tensorflow-lite",
      "features": [
        "google-angle"
      ],
      "platform": "windows"
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "opencv4"
  ],
  "features": {
    "opencv4": {
      "description": "Build with OpenCV 4",
      "dependencies": [
        "opencv4"
      ]
    }
  }
}
