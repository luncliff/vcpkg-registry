{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "test-self-hosted",
  "version-date": "2025-10-17",
  "description": "vcpkg registry maintained by @luncliff",
  "homepage": "https://github.com/luncliff/vcpkg-registry",
  "supports": "windows",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    }
  ],
  "features": {
    "cuda": {
      "description": "Install CUDA related ports",
      "supports": "x64 & windows",
      "dependencies": [
        "cuda",
        "cudnn",
        {
          "name": "llama-cpp",
          "features": [
            "cuda"
          ]
        },
        "magma",
        "nvidia-cudnn-frontend",
        {
          "name": "nvidia-cutlass",
          "features": [
            "tools"
          ]
        },
        "nvidia-tools-extension-sdk",
        {
          "name": "onnxruntime",
          "features": [
            "cuda"
          ]
        }
      ]
    },
    "host-tools": {
      "description": "install tool ports, with host: true option",
      "supports": "native",
      "dependencies": [
        {
          "name": "godot",
          "host": true,
          "features": [
            "spine-runtimes"
          ],
          "platform": "osx"
        },
        {
          "name": "godot",
          "host": true,
          "platform": "linux"
        },
        {
          "name": "llama-cpp",
          "features": [
            "tools"
          ]
        }
      ]
    },
    "test": {
      "description": "Default ports to test",
      "dependencies": [
        {
          "name": "d3d12-transition-layer",
          "features": [
            "pix"
          ],
          "platform": "x64 & windows"
        },
        {
          "name": "godot",
          "features": [
            "spine-runtimes"
          ],
          "platform": "x64 & windows"
        },
        "liblzma",
        {
          "name": "llama-cpp",
          "features": [
            "opencl",
            "openmp"
          ]
        },
        {
          "name": "onnx",
          "features": [
            "disable-static-registration",
            "test"
          ]
        },
        {
          "name": "onnxruntime",
          "features": [
            "openvino"
          ],
          "platform": "x64 & windows"
        },
        {
          "name": "winpixeventruntime",
          "platform": "x64 & windows"
        },
        {
          "name": "xnnpack",
          "host": true,
          "features": [
            "test"
          ]
        }
      ]
    },
    "vulkan": {
      "description": "Install Vulkan related ports",
      "supports": "x64 & windows",
      "dependencies": [
        "glslang",
        {
          "name": "llama-cpp",
          "features": [
            "vulkan"
          ]
        },
        "smol-v",
        "vulkan",
        "vulkan-headers"
      ]
    }
  },
  "overrides": [
    {
      "name": "fmt",
      "version": "9.1.0#1"
    },
    {
      "name": "flatbuffers",
      "version": "23.5.26#1"
    }
  ]
}
